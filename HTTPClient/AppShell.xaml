<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HTTPClient.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HTTPClient.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="HTTPClieet">

    <ShellContent
        Title="Posts"
        ContentTemplate="{DataTemplate local:PostView}"
        Route="PostView" />

    <ShellContent
        Title="Todos"
        ContentTemplate="{DataTemplate local:TodosView}"
        Route="TodosView" />
</Shell>
